
@{
    ViewData["Title"] = "Index";
}
@model List<MovieTheaterDto>
<h1>Index</h1>
 <table class="table table-striped">
        <thead>
            <tr>
                <td>Id</td>
                <td>Name</td>
                
            </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
                <tr>
                   <td>@item.Id</td>
                   <td>@item.Name</td>
                   
                </tr>
        }
        </tbody>
    </table>
